{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://godherancabackend2-a7sse79m.b4a.run/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://god-heranca-dashboardfrontend.vercel.app"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        }
      ]
    }
  ]
}
